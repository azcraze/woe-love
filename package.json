{
  "name": "woe-love",
  "version": "1.0.0",
  "description": "custom person module",
  "repository": {
    "type": "git",
    "url": "https://github.com/azcraze/woe-love.git"
  },
  "license": "MIT",
  "author": "azcraze",
  "main": "index.js",
  "scripts": {
    "changelog": "auto-changelog -p --template keepachangelog  --release-summary && git add CHANGELOG.md",
    "precommit": "npx prettier --write . && git add .",
    "commit": "npm run precommit && git-cz",
    "cz": "git-cz",
    "dev": "nodemon --exec \"npm run lint\"",
    "lint": "npx prettier --write . && eslint --fix .",
    "lint:full": "npx prettier --write . && npx eslint-interactive ./",
    "release": "dotenv -e .env -- semantic-release",
    "test": "npx majestic"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx semantic-release --dry-run && git add CHANGELOG.md package.json"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git",
      "useEmoji": false,
      "czConfig": "./.cz-config.js"
    }
  },
  "release": {
    "branches": [
      "main",
      "develop"
    ],
    "path": "./releaserc.js",
    "plugins": [
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ]
    ]
  },
  "dependencies": {
    "-": "^0.0.1",
    "@davidfig/json-depth": "^1.0.0",
    "@hero-page/hero-ai-package-creator": "^1.0.10",
    "@karmaniverous/dirtree": "^1.1.3",
    "@tectalic/openai": "^0.3.1",
    "1-liners": "^0.4.2",
    "app-root-path": "^3.1.0",
    "cwd": "^0.10.0",
    "futils": "^3.1.3",
    "gradient-string": "^2.0.2",
    "jsome": "^2.3.26",
    "jsonpath": "^1.1.1",
    "kyanite": "^1.6.3",
    "lowline.ai": "^1.0.18",
    "luxon": "^3.3.0",
    "openai": "^3.3.0",
    "path-resolver": "^0.0.2",
    "pretty-error": "^4.0.0",
    "relative": "^3.0.2",
    "requireindex": "^1.2.0",
    "rootpath": "^0.1.2",
    "typy": "^3.3.0",
    "winston": "^3.9.0",
    "woe-love": "^1.0.0",
    "woelogs": "^1.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^1.0.0",
    "ajv": "^8.12.0",
    "auto-changelog": "^2.4.0",
    "commitizen": "^1.0.3",
    "cz-git": "^1.6.1",
    "cz-gitmoji": "^0.0.7",
    "dotenv": "^16.3.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^25.0.0",
    "jsdoc-to-markdown": "^8.0.0",
    "prettier": "2.8.8",
    "semantic-release-gitmoji": "^1.6.4"
  }
}
